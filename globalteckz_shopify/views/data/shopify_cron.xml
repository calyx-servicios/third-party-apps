<data noupdate="1">

    <record forcecreate="True" id="shopify_cron" model="ir.cron">
        <field name="name">Shopify Cron</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field ref="model_gt_shopify_instance" name="model_id"/>
        <field name="state">code</field>
        <field name="code">model.cron_execute()</field>
        <field eval="False" name="doall"/>
        <field name="function">True</field>
    </record>
	
	<record forcecreate="True" id="shopify_cron_products" model="ir.cron">
        <field name="name">Shopify Cron Products</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">12</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field ref="model_gt_shopify_instance" name="model_id"/>
        <field name="state">code</field>
        <field name="code">model.cron_execute_products()</field>
        <field eval="False" name="doall"/>
        <field name="function">True</field>
    </record>
	
</data>